{
  "name": "@moveindustries/wallet-adapter-mui-design",
  "version": "5.4.3",
  "description": "Movement Wallet Adapter mui design",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "license": "Apache-2.0",
  "files": [
    "dist"
  ],
  "exports": {
    "types": "./dist/index.d.ts",
    "require": "./dist/index.js",
    "import": "./dist/index.mjs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MoveIndustries/movement-wallet-adapter.git"
  },
  "homepage": "https://github.com/MoveIndustries/movement-wallet-adapter",
  "bugs": {
    "url": "https://github.com/MoveIndustries/movement-wallet-adapter/issues"
  },
  "author": "movementlabs.xyz",
  "keywords": [
    "Movement",
    "Movement Labs",
    "Wallet",
    "Wallet Adapter",
    "Wallet Adapter Provider",
    "React"
  ],
  "scripts": {
    "build": "tsup src/index.tsx --format esm,cjs --dts --external react",
    "dev": "tsup src/index.tsx --format esm,cjs --watch --dts --external react",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "lint": "TIMING=1 eslint \"src/**/*.ts*\""
  },
  "devDependencies": {
    "@moveindustries/wallet-adapter-tsconfig": "workspace:*",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "eslint": "^8.57.1",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@moveindustries/wallet-adapter-react": "workspace:*",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.11.6"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "react-router-dom": "^7.5.2"
  }
}

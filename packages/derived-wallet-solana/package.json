{
  "name": "@moveindustries/derived-wallet-solana",
  "version": "0.8.3",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/types/index.d.ts",
  "license": "Apache-2.0",
  "exports": {
    "types": "./dist/types/index.d.ts",
    "require": "./dist/index.js",
    "import": "./dist/index.mjs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MoveIndustries/movement-wallet-adapter.git"
  },
  "homepage": "https://github.com/MoveIndustries/movement-wallet-adapter",
  "bugs": {
    "url": "https://github.com/MoveIndustries/movement-wallet-adapter/issues"
  },
  "author": "movementlabs.xyz",
  "scripts": {
    "build": "pnpm build:bundle && pnpm build:declarations",
    "build:bundle": "tsup --sourcemap",
    "build:declarations": "tsc --emitDeclarationOnly --declaration --declarationMap",
    "dev": "tsup src/index.ts --format esm,cjs --watch",
    "test": "jest",
    "lint": "TIMING=1 eslint \"src/**/*.ts*\""
  },
  "tsup": {
    "entryPoints": [
      "src/index.ts"
    ],
    "format": [
      "cjs",
      "esm"
    ]
  },
  "dependencies": {
    "@moveindustries/derived-wallet-base": "workspace:^",
    "@aptos-labs/siwa": "^0.4.0",
    "@moveindustries/wallet-standard": "^0.6.0",
    "@solana/wallet-adapter-base": "^0.9.23",
    "@solana/wallet-standard-util": "^1.1.2",
    "@solana/wallet-standard-wallet-adapter-base": "^1.1.4",
    "@solana/web3.js": "^1.95.8",
    "@wallet-standard/app": "^1.1.0"
  },
  "peerDependencies": {
    "@moveindustries/ts-sdk": "^5.1.6"
  },
  "devDependencies": {
    "@moveindustries/eslint-config-adapter": "workspace:*",
    "@moveindustries/wallet-adapter-tsconfig": "workspace:*",
    "@types/jest": "^29.2.4",
    "@types/node": "^20.10.4",
    "eslint": "^8.57.1",
    "jest": "^29.3.1",
    "ts-jest": "^29.0.3",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3"
  },
  "files": [
    "dist",
    "src",
    "!src/**.test.ts",
    "!src/**/__tests__"
  ]
}
